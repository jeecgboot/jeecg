			<name>fileNumLimit</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>最大文件数默认3</description>
		</attribute>
		<attribute>
			<name>fileSingleSizeLimit</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
