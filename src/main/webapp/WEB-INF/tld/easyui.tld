			<name>duplicate</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>去重， 根据文件名字、文件大小和最后修改时间来生成hash Key</description>
		</attribute>
		<attribute>
			<name>fileVal</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>设置文件上传域的name</description>
		</attribute>
		<attribute>
			<name>extendParams</name>
			<required>false</required>
			<description>类似css写法 这是每个文件上传时候需要传递的参数字符串应带上引号</description>
		</attribute>
		<attribute>
			<name>url</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>文件处理action</description>
		</attribute>
		<attribute>
			<name>datatype</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>只要该属性有值,均视之为不为空</description>
		</attribute>
		<attribute>
			<name>nullMsg</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>为空时候的提示信息</description>
		</attribute>
		<attribute>
			<name>bizType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>业务类型[StoreUploadFilePathEnum],根据该类型确定上传路径</description>
		</attribute>
		<attribute>
			<name>displayTxt</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示上传列表[默认显示]true显示false隐藏</description>
		</attribute>
		<!-- update-begin- Author:taoyan  Date:20180519 for：文件上传增加属性：是否在外部引入js-css  -->
		<attribute>
			<name>outJs</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否在外部引入js-css</description>
		</attribute>
		<!-- update-end- Author:taoyan  Date:20180519 for：文件上传增加属性：是否在外部引入js-css  -->
