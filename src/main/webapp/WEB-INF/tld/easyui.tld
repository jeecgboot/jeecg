<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
                        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	<short-name>t</short-name>
	<uri>/easyui-tags</uri>
	<display-name>"自定义标签"</display-name>
	
	<tag>
        <name>mutiLang</name>
        <tag-class>org.jeecgframework.tag.core.easyui.MutiLangTag</tag-class>
        <body-content>JSP</body-content>
        <description>多语言输出</description>
        <attribute>
            <name>langKey</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <description>控件名称</description>
        </attribute>
        
        <attribute>
            <name>langArg</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description>参数</description>
        </attribute>
    </tag>
	
	<tag>
		<name>base</name>
		<tag-class>org.jeecgframework.tag.core.easyui.BaseTag</tag-class>
		<body-content>JSP</body-content>
		<description>父类标签生成JS CSS</description>
		<attribute>
			<name>type</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>加载类型</description>
		</attribute>
		<attribute>
			<name>cssTheme</name>
			<rtexprvalue>true</rtexprvalue>
			<description>easyui theme（目录 默认default）</description>
		</attribute>
	</tag>
	<tag>
		<name>datagrid</name>
		<tag-class>org.jeecgframework.tag.core.easyui.DataGridTag</tag-class>
		<body-content>JSP</body-content>
		<description>数据列表</description>
		<!-- update-begin author:xuelin date:20171116 for:TASK #2404 【平台UI改造】UI样式改造点  5.参考fineui，做下字段过滤功能效果 -->
		<attribute>
			<name>filter</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否启用过滤</description>
		</attribute>
		<!-- update-end author:xuelin date:20171116 for:TASK #2404 【平台UI改造】UI样式改造点  5.参考fineui，做下字段过滤功能效果 -->
		<!-- update-begin-Author:xuelin  Date:20170523 for：TASK #2002 【UI优化】列表上方button样式改造方案  -->	
		<attribute>
			<name>btnCls</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>列表上方button样式class属性</description>
		</attribute>
		<!-- update-end-Author:xuelin  Date:20170523 for：TASK #2002 【UI优化】列表上方button样式改造方案  -->	
		<!-- update-begin-author:zhangjiaqiang  Date:20170626 for:TASK #2168 【UI改造】这里能显示多行文本 -->
		<attribute>
			<name>nowrap</name>
			<rtexprvalue>true</rtexprvalue>
			<description>列表内容是否可换行(false：可换行)</description>
		</attribute>
		<!-- update-end-author:zhangjiaqiang  Date:20170626 for:TASK #2168 【UI改造】这里能显示多行文本 -->
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>列表TABLE标示</description>
		</attribute>
		<attribute>
			<name>treegrid</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否是树形列表 值为true 或者false</description>
		</attribute>
		<attribute>
			<name>actionUrl</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>分页提交的路径</description>
		</attribute>
		<attribute>
			<name>pagination</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否分页 true,false</description>
		</attribute>
		<attribute>
			<name>title</name>
			<rtexprvalue>true</rtexprvalue>
			<description>表格标题</description>
		</attribute>
		<attribute>
			<name>idField</name>
			<rtexprvalue>true</rtexprvalue>
			<description>主键字段</description>
		</attribute>
		<attribute>
			<name>treeField</name>
			<rtexprvalue>true</rtexprvalue>
			<description>树形列表展示列</description>
		</attribute>
		<attribute>
			<name>width</name>
			<rtexprvalue>true</rtexprvalue>
			<description>表格宽度</description>
		</attribute>
		<attribute>
			<name>height</name>
			<rtexprvalue>true</rtexprvalue>
			<description>表格高度</description>
		</attribute>
		<attribute>
			<name>checkbox</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示复选框</description>
		</attribute>
		<attribute>
			<name>fit</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否适应父容器,true 默认 ,false</description>
		</attribute>
		<attribute>
			<name>sortName</name>
			<rtexprvalue>true</rtexprvalue>
			<description>定义的列进行排序</description>
		</attribute>
		<attribute>
			<name>sortOrder</name>
			<rtexprvalue>true</rtexprvalue>
			<description>定义列的排序顺序，只能是"递增 asc"或"降序desc"</description>
		</attribute>
		<attribute>
			<name>fitColumns</name>
			<rtexprvalue>true</rtexprvalue>
			<description>当为true时，自动展开/合同列的大小，以适应的宽度，防止横向滚动</description>
		</attribute>
		<attribute>
			<name>collapsible</name>
			<rtexprvalue>true</rtexprvalue>
			<description>当为true时，开启收起/展开，默认不启动</description>
		</attribute>
		<attribute>
			<name>showPageList</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示页面列表</description>
		</attribute>
		<attribute>
			<name>showRefresh</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示刷新按钮</description>
		</attribute>
		<attribute>
			<name>showText</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示分页信息</description>
		</attribute>
		<attribute>
			<name>style</name>
			<rtexprvalue>true</rtexprvalue>
			<description>表格样式easyui,datatables</description>
		</attribute>
		<attribute>
			<name>pageSize</name>
			<rtexprvalue>true</rtexprvalue>
			<description>每页显示条数</description>
		</attribute>
		<attribute>
			<name>onLoadSuccess</name>
			<rtexprvalue>true</rtexprvalue>
			<description>数据加载成调用方法</description>
		</attribute>
		<attribute>
			<name>onDblClick</name>
			<rtexprvalue>true</rtexprvalue>
			<description>双击调用方法</description>
		</attribute>
		<attribute>
			<name>onClick</name>
			<rtexprvalue>true</rtexprvalue>
			<description>单击调用方法 tree 下function(rowData),dataGrid
				下function(rowIndex,rowData)</description>
		</attribute>
		<attribute>
			<name>queryMode</name>
			<rtexprvalue>true</rtexprvalue>
			<description>查询模式：single(单条件查询：默认),group(组合查询),advanced(高级查询)</description>
		</attribute>
		<attribute>
			<name>autoLoadData</name>
			<rtexprvalue>true</rtexprvalue>
			<description>列表页面数据加载模式。true自动加载数据，false手动加载，默认为true</description>
		</attribute>
		<attribute>
			<name>openFirstNode</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是不是展开第一个节点,在树形情况下,true展开,false不展开默认false</description>
		</attribute>
		<attribute>
			<name>entityName</name>
			<rtexprvalue>true</rtexprvalue>
			<description>对应的实体对象,如果entity和controller都是规则的可以不填,自动补全标签做关联</description>
		</attribute>
		<attribute>
			<name>rowStyler</name>
			<rtexprvalue>true</rtexprvalue>
			<description>行 css函数 指定名称就可以,调用为 functionName(index,row)这样调用</description>
		</attribute>
		<attribute>
			<name>extendParams</name>
			<rtexprvalue>true</rtexprvalue>
			<description>datagrid 的扩展字段,如果easyui上面有但是jeecg没有这个属性可以自己添加
				添加规则和easyui的field一致</description>
		</attribute>
		<attribute>
			<name>queryBuilder</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否使用高级查询器true,false</description>
		</attribute>
		<!-- update-begin-Author:LiShaoQing  Date:20171019 for：TASK #2356 【功能】添加新的高级查询按钮功能 -->
		<attribute>
			<name>superQuery</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否使用高级查询器true,false</description>
		</attribute>
		<!-- update-end-Author:LiShaoQing  Date:20171019 for：TASK #2356 【功能】添加新的高级查询按钮功能 -->
		<!-- update-begin-Author:LiShaoQing  Date:20171227 for：添加新的高级查询构造器按钮功能 -->
		<attribute>
			<name>complexSuperQuery</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否使用高级查询器构造器,值不为空显示高级查询构造器按钮</description>
		</attribute>
		<!-- update-end-Author:LiShaoQing  Date:20171227 for：添加新的高级查询构造器按钮功能 -->
		<attribute>
			<name>singleSelect</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否单选true,false</description>
		</attribute>
		<attribute>
			<name>isShowSearch</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否使用检索可收缩功能true,false</description>
		</attribute>
		<!-- update-begin-Author:guoxianhui  Date:20171214 for：TASK #2450 【改造】支持主子表效果 -->
		<attribute>
			<name>isShowSubGrid</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示表体数据 值为true 或者false</description>
		</attribute>
		<attribute>
			<name>configId</name>
			<rtexprvalue></rtexprvalue>
			<description>主表表名</description>
		</attribute>
		<!-- update-end-Author:guoxianhui  Date:20171214 for：TASK #2450 【改造】支持主子表效果 -->
		<!-- update-begin-Author:zhoujf  Date:20180504 for：TASK #2687 【datagrid标签】使用freemaker重写datagrid标签 -->
		<attribute>
			<name>component</name>
			<rtexprvalue></rtexprvalue>
			<description>列表组件名称（默认easyui,bootstrap-table）</description>
		</attribute>
		<attribute>
			<name>query</name>
			<rtexprvalue></rtexprvalue>
			<description>是否显示查询条件（默认true,显示为true,不显示为false）</description>
		</attribute>
		<!-- update-end-Author:zhoujf  Date:20180504 for：TASK #2687 【datagrid标签】使用freemaker重写datagrid标签 -->
		<!-- update-begin-Author:taoYan  Date:20180522 for:是否开启按钮过滤模式 -->
		<attribute>
			<name>filterBtn</name>
			<rtexprvalue></rtexprvalue>
			<description>是否开启按钮过滤模式，调用指定函数(函数名:name+"Filter")出现过滤行</description>
		</attribute>
		<!-- update-end-Author:taoYan  Date:20180522 for:是否开启按钮过滤模式 -->
	</tag>
	<tag>
		<name>dgCol</name>
		<tag-class>org.jeecgframework.tag.core.easyui.DataGridColumnTag</tag-class>
		<body-content>JSP</body-content>
		<description>列表字段</description>
		<attribute>
			<name>title</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>字段标题</description>
		</attribute>
		
		<!-- update-begin-Author:xuelin  Date:20170712 for：TASK #2205 【UI标签库】列表查询条件动态生成，下拉换成redio模式切换 -->
		<attribute>
			<name>showMode</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>列表查询字段生成模式：radio/checkbox/select</description>
		</attribute>
		<!-- update-end-Author:xuelin  Date:20170712 for：TASK #2205 【UI标签库】列表查询条件动态生成，下拉换成redio模式切换 --> 
		<attribute>
			<name>langArg</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>多语言参数</description>
		</attribute>

		<attribute>
			<name>field</name>
			<rtexprvalue>true</rtexprvalue>
			<description>字段值</description>
		</attribute>
		<attribute>
			<name>width</name>
			<rtexprvalue>true</rtexprvalue>
			<description>字段宽度</description>
		</attribute>
		<attribute>
			<name>showLen</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>显示长度，超出省略号显示</description>
		</attribute>
		<attribute>
			<name>rowspan</name>
			<rtexprvalue>true</rtexprvalue>
			<description>字段跨列</description>
		</attribute>
		<attribute>
			<name>colspan</name>
			<rtexprvalue>true</rtexprvalue>
			<description>字段跨行</description>
		</attribute>
		<attribute>
			<name>align</name>
			<rtexprvalue>true</rtexprvalue>
			<description>数据对齐方式，可选值有left,right,center</description>
		</attribute>
		<attribute>
			<name>sortable</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否排序</description>
		</attribute>
		<attribute>
			<name>checkbox</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示选择框</description>
		</attribute>
		<attribute>
			<name>formatter</name>
			<rtexprvalue>true</rtexprvalue>
			<description>自定义函数名称</description>
		</attribute>
		<attribute>
			<name>formatterjs</name>
			<rtexprvalue>true</rtexprvalue>
			<description>自定义函数名称(调用页面自定义js方法 参数为(value,row,index)</description>
		</attribute>
		<attribute>
			<name>hidden</name>
			<rtexprvalue>true</rtexprvalue>
			<description>列隐藏(隐藏:false)</description>
		</attribute>
		<attribute>
			<name>replace</name>
			<rtexprvalue>true</rtexprvalue>
			<description>字段值替换(格式 ：old_new,old2_new2)</description>
		</attribute>
		<attribute>
			<name>treefield</name>
			<rtexprvalue>true</rtexprvalue>
			<description>树形菜单对应字段</description>
		</attribute>
		<attribute>
			<name>image</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示图片字段</description>
		</attribute>
		<attribute>
			<name>imageSize</name>
			<rtexprvalue>true</rtexprvalue>
			<description>自定义大小显示(格式：width,height)</description>
		</attribute>
		<attribute>
			<name>query</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否为添加查询列</description>
		</attribute>
		<attribute>
			<name>queryMode</name>
			<rtexprvalue>true</rtexprvalue>
			<description>字段查询模式：single(默认：单字段查询),group(范围查询)</description>
		</attribute>
		<attribute>
			<name>frozenColumn</name>
			<rtexprvalue>false</rtexprvalue>
			<description>是否是冰冻列。默认false</description>
		</attribute>
		<attribute>
			<name>funname</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>自定义函数</description>
		</attribute>
		<attribute>
			<name>url</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>自定义链接地址</description>
		</attribute>
		<attribute>
			<name>dictionary</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>数据字典组编码 或 自定义字典(格式：表名,编码,显示文本)</description>
		</attribute>
		<!-- update-begin-Author:gj_shaojc  Date:20180314 for：TASK #2560 【UI标签扩展】column列表字典属性扩展dictCondition加sql条件  -->
		<attribute>
			<name>dictCondition</name>
			<rtexprvalue>true</rtexprvalue>
			<description>自定义字典表的显示文本-字典查询条件</description>
		</attribute>
		<!-- update-end-Author:gj_shaojc  Date:20180314 for：TASK #2560 【UI标签扩展】column列表字典属性扩展dictCondition加sql条件  -->
		<attribute>
			<name>extend</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>扩展属性(采用json格式)查询的时候使用</description>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>td CSS 属性</description>
		</attribute>
		<attribute>
			<name>downloadName</name>
			<rtexprvalue>true</rtexprvalue>
			<description>附件下载</description>
		</attribute>
		<attribute>
			<name>autocomplete</name>
			<rtexprvalue>true</rtexprvalue>
			<description>自动补全,true 自动补全,false不补全,默认false 填写当前字段</description>
		</attribute>
		<attribute>
			<name>extendParams</name>
			<rtexprvalue>true</rtexprvalue>
			<description>datagrid field 的扩展字段,如果easyui上面有但是jeecg没有这个属性可以自己添加
				添加规则和easyui的field一致</description>
		</attribute>
  		<attribute>
   			<name>popup</name>
   			<rtexprvalue>true</rtexprvalue>
   			<description>是否启用popup模式</description>
  		</attribute>
  		<attribute>
   			<name>editor</name>
   			<rtexprvalue>true</rtexprvalue>
   			<description>高级查询器用的字段编辑器</description>
  		</attribute>
  		<attribute>
   			<name>defaultVal</name>
   			<rtexprvalue>true</rtexprvalue>
   			<description>UI标签库列表默认查询条件</description>
  		</attribute>
  		<attribute>
  			<name>newColumn</name>
  			<rtexprvalue>true</rtexprvalue>
  			<description>是否另起一行</description>
  		</attribute>
  		<!-- update-begin-Author:taoYan  Date:20180522 for:船舶风格列属性扩展 -->
  		<attribute>
  			<name>filterType</name>
  			<rtexprvalue>true</rtexprvalue>
  			<description>过滤操作的类型num/combobox/date/datetime/text 默认text</description>
  		</attribute>
  		<attribute>
  			<name>optsMenu</name>
  			<rtexprvalue>true</rtexprvalue>
  			<description>opt列是否有隐藏菜单</description>
  		</attribute>
  		<!-- update-end-Author:taoYan  Date:20180522 for:船舶风格列属性扩展 -->
  		<attribute>
			<name>isAjaxDict</name>
			<rtexprvalue></rtexprvalue>
			<description>是否开启ajax方式获取字典信息（默认false,开启为true,不开启为false）</description>
		</attribute>
	</tag>
	<tag>
		<name>dgDelOpt</name>
		<tag-class>org.jeecgframework.tag.core.easyui.DataGridDelOptTag</tag-class>
		<body-content>JSP</body-content>
		<description>列表删除操作</description>
		<attribute>
			<name>url</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>列表操作URL</description>
		</attribute>
		<attribute>
			<name>title</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>操作标题</description>
		</attribute>
		
		<attribute>
			<name>langArg</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>多语言参数</description>
		</attribute>

		<attribute>
			<name>message</name>
			<rtexprvalue>true</rtexprvalue>
			<description>询问内容</description>
		</attribute>
		<attribute>
			<name>exp</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示改链接的表达式,例 字段名#表达式符号#字段值(name#eq#admin)
